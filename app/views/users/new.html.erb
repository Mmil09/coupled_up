<%= bootstrap_form_for(@user) do |f| %>
<%= f.text_field :username %>
<%= f.email_field :email %>
<%= f.text_field :first_name %>
<%= f.text_field :last_name %>
<%= f.text_field :city %>
<%= f.text_field :state %>
<%= f.text_field :zip_code %>
<%= f.text_area :about %>
<%= f.password_field :password %>
<%= f.date_select :birthday %>

<%= f.submit %>
<% end %>