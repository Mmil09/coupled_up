<section>
            <div id="carousel-example-generic" class="carousel carousel-mind slide" data-ride="carousel" data-interval="5000">
                <!-- Indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                    <li data-target="#carousel-example-generic" data-slide-to="1" class=""></li>
                    <li data-target="#carousel-example-generic" data-slide-to="2" class=""></li>
                </ol>

                <!-- Wrapper for slides -->
                <div class="carousel-inner">
                    <div class="item active">
                        <%= image_tag("Activities/Kayaking.jpg") %>
                        <div class="container">
                          <div class="carousel-caption">
                            <p>Feeling adventurous?  Come and take a kayaking trip with Outdoor Sports.</p>
                          </div>
                        </div>
                      </div>

                      <div class="item">
                        <%= image_tag("Activities/Painting-group-banner1.jpg") %>
                        <div class="container">
                          <div class="carousel-caption">
                            <p>Explore your artistic side while enjoying nice weather at Franklin Park.</p>
                          </div>
                        </div>
                      </div>

                      <div class="item">
                        <%= image_tag("Activities/the-lizard-lounge-91.jpeg") %>
                        <div class="container">
                          <div class="carousel-caption">
                            <p>Come to Karaoke night at Lizard Lounge for a fun night of singing and dancing.</p>
                          </div>
                        </div>
                      </div>
                </div>

                <!-- Controls -->
                <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left"></span>
                </a>
                <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right"></span>
                </a>
            </div>
        </section> <!-- carousel -->